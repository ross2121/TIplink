generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User{
  id String  @id @default(uuid()) 
  name String
  email String @unique
  picture String
  sub  String  @default("")
  SQLWALLET SOLWALLET?
  InrWallet Inrwallet?
   provider String?

}
model Inrwallet{
  id Int @id @default(autoincrement())
  balance Int
  userId   String @unique
  User    User @relation(fields: [userId],references: [id])
}
model SOLWALLET{
  id Int @id @default(autoincrement())
  userId String @unique
   publiclkey String?
  privateKey String?
  User User @relation(fields: [userId],references: [id])
}
